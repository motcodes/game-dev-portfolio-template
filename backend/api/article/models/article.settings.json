{
  "kind": "collectionType",
  "collectionName": "project",
  "info": {
    "name": "Projects",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "heroImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "projectYearDate": {
      "type": "string",
      "required": true,
      "default": "2021"
    },
    "myRoles": {
      "type": "component",
      "repeatable": true,
      "component": "shared.role"
    },
    "credits": {
      "type": "component",
      "repeatable": true,
      "component": "shared.credits-role"
    },
    "contentGroup": {
      "type": "dynamiczone",
      "components": [
        "shared.image",
        "shared.content",
        "shared.image-grid",
        "shared.fullsize-image"
      ]
    },
    "projectLink": {
      "type": "component",
      "repeatable": false,
      "component": "shared.social-media-link"
    }
  }
}
